---
import Layout from "../layouts/Layout.astro";
import {
  Navigation,
  Hero,
  About,
  Expertise,
  OurWorks,
  EndUsers,
  Team,
  Career,
  Contact,
  Footer,
  InteractiveBackground,
} from "../components";
---

<Layout>
  <InteractiveBackground client:load />
  <Navigation client:load />
  <Hero client:load />
  <About client:load />
  <Expertise client:idle />
  <OurWorks client:visible />
  <EndUsers client:visible />
  <Team client:visible />
  <Career client:visible />
  <Contact client:visible />
  <Footer />
</Layout>
